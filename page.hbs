{{!< default}}

{{! The tag above means: insert everything in this file
into the {body} tag of the default.hbs template }}

{{#post}}

  <img
    class='q-post-article-header-image'
    src='{{img_url feature_image}}'
    loading='lazy'
  />
  <article class='q-post-article-inner'>
    <header class='q-post-article-header'>
      <h1 class='q-post-article-title'>{{title}}</h1>
    </header>
    <section class='q-post-article-content'>
      {{content}}
    </section>

  </article>

{{/post}}